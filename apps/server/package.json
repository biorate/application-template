{
  "name": "server",
  "version": "0.0.0",
  "description": "Application server template",
  "main": "dist",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "build": "npx tsc -p ./tsconfig.build.json --outDir ./dist",
    "start:debug": "cross-env NODE_ENV=debug nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "npx nyc --reporter=lcov --reporter=text-summary -- npx mocha --exit -r ts-node/register tests/**/*.spec.ts"
  },
  "keywords": [],
  "author": "llevkin",
  "license": "UNLICENSED",
  "dependencies": {
    "@biorate/file-config": "^0.18.1",
    "@biorate/prometheus": "^0.18.0",
    "@nestjs/common": "^7.6.17",
    "@nestjs/core": "^7.6.17",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^4.8.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "helmet": "^4.6.0",
    "response-time": "^2.3.2",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/cookie-parser": "^1.4.2",
    "nodemon": "^2.0.7"
  }
}
