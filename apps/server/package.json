{
  "name": "server",
  "version": "0.0.0",
  "description": "Application server template",
  "main": "dist",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "build": "npx tsc -p ./tsconfig.build.json --outDir ./dist",
    "start:debug": "cross-env ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "npx nyc -- npx mocha",
    "lint:fix": "npx eslint --fix ./src",
    "prettier:fix": "npx prettier --write ./src",
    "allure:report": "npx allure serve allure-results --clean -o allure-report",
    "hooks:pre-commit": "pnpm run prettier:fix && pnpm run lint:fix"
  },
  "keywords": [],
  "author": "llevkin",
  "license": "UNLICENSED",
  "dependencies": {
    "@biorate/axios-prometheus": "^1.54.0",
    "@biorate/config-loader": "^1.54.0",
    "@biorate/config-loader-env": "^1.54.0",
    "@biorate/config-loader-fs": "^1.54.0",
    "@biorate/config-loader-vault": "^1.54.0",
    "@biorate/nestjs-tools": "^1.54.0",
    "@biorate/prometheus": "^1.54.0",
    "@biorate/vault": "^1.54.0",
    "@nestjs/common": "^9.3.12",
    "@nestjs/core": "^9.3.12",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/platform-ws": "^9.3.12",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/websockets": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.18.2",
    "helmet": "^4.6.0",
    "http-proxy-middleware": "^2.0.6",
    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.6.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@biorate/mocha": "^1.57.0",
    "@biorate/mocha-spec": "^1.53.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/mocha": "^9.1.1",
    "@types/ws": "^8.5.8",
    "mocha": "^10.2.0",
    "mocha-chai-jest-snapshot": "^1.1.3",
    "mocha-multi-reporters": "^1.5.1"
  }
}
