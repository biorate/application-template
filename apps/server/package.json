{
  "name": "server",
  "version": "0.0.0",
  "description": "Application server template",
  "main": "dist",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "build": "npx tsc -p ./tsconfig.build.json --outDir ./dist",
    "start:debug": "cross-env NODE_ENV=debug nodemon --watch 'src/**/*.ts' --exec 'ts-node-dev' src/index.ts",
    "test": "npx nyc --reporter=lcov --reporter=text-summary -- npx mocha --exit -r ts-node/register tests/**/*.spec.ts"
  },
  "keywords": [],
  "author": "llevkin",
  "license": "UNLICENSED",
  "dependencies": {
    "@biorate/file-config": "^0.26.0",
    "@biorate/prometheus": "^0.26.0",
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "helmet": "^5.0.2",
    "on-headers": "^1.0.2",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.16"
  }
}
