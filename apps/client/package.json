{
  "name": "client",
  "version": "0.0.0",
  "description": "app client template",
  "main": "dist",
  "scripts": {
    "build": "npx webpack --config=../../webpack.config.js --mode=production --entry=./src/index.tsx --entry=./src/index.pug --entry=./src/index.less -o ./dist",
    "build:debug": "npx webpack serve --history-api-fallback --config=../../webpack.config.js --mode=development --hot --open --entry=./src/index.tsx --entry=./src/index.pug --entry=./src/index.less -o ./dist",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:ls",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:stylelint -- --fix && npm run lint:ls",
    "lint:stylelint": "stylelint \"src/**/*.{ts,tsx,pcss}\"",
    "lint:stylelint:report": "stylelint \"src/**/*.{ts,tsx,pcss}\" --output-file reports/stylelint-report.txt",
    "lint:eslint": "eslint ./src --ext .ts,.tsx",
    "lint:eslint:report": "eslint ./src --ext .ts,.tsx --output-file reports/eslint-report.json --format json",
    "lint:ls": "ls-lint"
  },
  "keywords": [],
  "author": "llevkin",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "classnames": "^2.3.1",
    "history": "^5.3.0",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1"
  }
}
