{
  "name": "application-template",
  "private": true,
  "version": "0.0.0",
  "description": "Application template (Nestjs + React + Inversify)",
  "scripts": {
    "build": "npx lerna run build --stream --no-prefix --scope client --scope server",
    "start": "npx lerna run start --stream --no-prefix --scope server",
    "build:debug": "npx lerna run build:debug --stream --no-prefix --scope client",
    "start:debug": "npx lerna run start:debug --stream --no-prefix --scope server",
    "test": "npx lerna run test --stream --no-prefix",
    "bootstrap": "npx lerna bootstrap",
    "version": "npx lerna version",
    "postinstall": "npm run bootstrap",
    "lint": "npx lerna run lint --stream --no-prefix --scope client",
    "cleanup:node_modules": "rm -rf $(find -type d -name 'node_modules')",
    "cy:open": "npx lerna run cy:open --stream --no-prefix --scope client",
    "cy:run": "npx lerna run cy:run --stream --no-prefix --scope client",
    "allure:report": "npx lerna run allure:report --stream --no-prefix --scope client"
  },
  "keywords": [],
  "author": "llevkin",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/chai": "^4.3.1",
